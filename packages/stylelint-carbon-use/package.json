{
  "name": "stylelint-carbon-use",
  "description": "A stylelint plugin to support the use of carbon component tokens.",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "repository": "https://github.com/carbon-design-system/carbon/tree/master/packages/stylelint-carbon-use",
  "bugs": "https://github.com/carbon-design-system/carbon/issues",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "docs",
    "src/**/README.md",
    "!**/__tests__"
  ],
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "css",
    "carbon",
    "scss",
    "ibm",
    "carbon-design-system",
    "components",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist"
  },
  "peerDependencies": {},
  "dependencies": {
    "@carbon/colors": "^10.13.0",
    "@carbon/motion": "^10.9.0",
    "@carbon/themes": "^10.18.0",
    "carbon-components": "^10.19.0",
    "postcss": "^7.0.27",
    "postcss-media-query-parser": "^0.2.3",
    "postcss-resolve-nested-selector": "^0.1.1",
    "postcss-scss": "^2.0.0",
    "postcss-selector-parser": "^6.0.2",
    "postcss-value-parser": "^4.0.3"
  },
  "devDependencies": {
    "jest": "^25.3.0",
    "jest-cli": "^25.3.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.3.2"
  }
}
